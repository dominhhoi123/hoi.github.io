function getQuerystring(n,t){t==null&&(t="");n=n.replace(/[\[]/,"\\[").replace(/[\]]/,"\\]");var r=new RegExp("[\\?&]"+n+"=([^&#]*)"),i=r.exec(window.location.href);return i==null?t:i[1]}function Tracking(){setTimeout(function(){$.getScript("//wurfl.io/wurfl.js").done(function(){var u=0,n,i;WURFL.is_mobile==!0&&(u=1);var e=WURFL.form_factor,o=WURFL.complete_device_name,s=encodeURIComponent(document.URL),r=encodeURIComponent(getQuerystring("key")),t=-1,f="";typeof GL_CATEGORYID!="undefined"&&(t=GL_CATEGORYID);t==-1&&$("#hdCategoryID").length>0&&(t=$("#hdCategoryID").val());n=-1;typeof ProductID!="undefined"&&(n=ProductID);(n==0||n==-1)&&typeof GL_ProductID!="undefined"&&(n=GL_ProductID);n==-1&&typeof productid!="undefined"&&(n=productid);r==undefined&&(r="");i=encodeURIComponent(document.referrer);i==undefined&&(i="");typeof mysessionid!="undefined"&&(f=mysessionid);var h=window.innerHeight,c=window.innerWidth,l=c+"x"+h,a=tgddctr_urlroot+"/tracking/load?urlrefer="+i+"&key="+r+"&categoryid="+t+"&productid="+n+"&sessionid="+f+"&pageurl="+s+"&ismobile="+u+"&device="+o+"&screen="+l+"&factory="+e,v='<img width="0" heigth="0" id="imgtrack" src="'+a+'" />';$("body").append(v)})},5e3)}function ActiveLeftMenuKNH(){try{$(".boxleft").length&&($(".boxleft a.active_l").parents("li").find("div").show(),$(".boxleft a.active_l").parents("li").find(".iconrig").hasClass("iconrigup")?$(".boxleft a.active_l").parents("li").find(".iconrig").removeClass("iconrigup"):$(".boxleft a.active_l").parents("li").find(".iconrig").addClass("iconrigup"))}catch(n){}}function showleftcate(n){$(n).find(".iconrig").hasClass("iconrigup")?$(n).find(".iconrig").removeClass("iconrigup"):$(n).find(".iconrig").addClass("iconrigup");$(n).next(".div_sublcate").toggle(100);$(window).resize()}function loadComment(){try{var n=document.createElement("script");n.type="text/javascript";n.async=!0;n.defer=!0;n.src=comment_cdn+"/Scripts/comment"+staticversion+".js";(document.getElementsByTagName("head")[0]||document.ge