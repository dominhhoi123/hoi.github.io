ody");setTimeout(function(){showUserLogin()},450)}function RemoveSessionOTP(){$.ajax({url:"/my-profile/aj/Auth/RemoveSessionOTP",type:"POST",cache:!0,data:{},beforeSend:function(){},success:function(){}})}function InitNavNotify(){$(".personal-login .showbox-notify").click(function(){$(".notifyfade").not(".allnotify").toggle()});isMobile&&(callAjaxNotify(),$(".personal-login .showbox-notify").click(function(){$(".subnav.v6").slideUp();$(".closeover").slideUp(450);setTimeout(function(){$(".popup-notify").show();$(".popup-notify .notifyfade").show()},500)}),$(".popup-notify .overlay").click(function(){$(".popup-notify").hide()}),$(".popup-notify .iconnews-closePopupNotify").click(function(){$(".popup-notify").hide()}))}function BeforeSendAjax(){$(".overlayloading").show()}function EndSendAjax(){$(".overlayloading").hide()}function ErrorAjax(){$(".overlayloading").hide()}function OpenLoginWindowCampaign(n){var r=host+rootUrl+"/auth?socialType="+n,t=screen.availWidth*.8,i=screen.availHeight*.6,u=(screen.availWidth-t)/2,f=(screen.availHeight-i)/2;window.socialWindow=window.open(r,"SignIn","width="+t+",height="+i+",toolbar=no,resizable=fixed,status=no,scrollbars=no,menubar=no,screenX="+u+",screenY="+f);socialWindow.focus();showFormUpdateAcount();return}function showUserLogin(n){$.ajax({url:"/my-profile/aj/Home/showUserLogin",type:"POST",cache:!0,data:{projectname:n},beforeSend:function(){$(".overlayloading").show()},success:function(n){$(".overlayloading").hide();$(".box-login").html(n);$(".box-login").show();InitEnterKeyLoginByPhone()},error:function(){}})}function showSussesUserUpdate(){$(".box-container").addClass("success");$(".box-container .relate").html("<div class='title'><i class='iconnews-loginsuccess'><\/i><\/div><h3>ĐĂNG KÝ THÀNH CÔNG<\/h3><a class='closePopup' onclick='hidePopupUpdate()'>Đóng<\/a><span class='close-popup' onclick='hidePopupUpdate()'><i class='iconnews-closegal'><\/i><\/span>")}function closePopupLogin(){$(".box-login").hide()}function showForm